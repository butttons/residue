{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "residue",
	"main": "src/index.tsx",
	"compatibility_date": "2025-02-14",
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "residue-db",
			"database_id": "YOUR_D1_DATABASE_ID",
			"migrations_dir": "migrations"
		}
	],
	"r2_buckets": [
		{
			"binding": "BUCKET",
			"bucket_name": "residue-sessions"
		}
	],
	"compatibility_flags": [
		"nodejs_compat"
	]
}
